<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<featureModel>
	<properties>
		<graphics key="showhiddenfeatures" value="true"/>
		<graphics key="legendautolayout" value="true"/>
		<graphics key="showshortnames" value="false"/>
		<graphics key="layout" value="vertical"/>
		<graphics key="showcollapsedconstraints" value="true"/>
		<graphics key="legendhidden" value="false"/>
		<graphics key="layoutalgorithm" value="4"/>
	</properties>
	<struct>
		<and abstract="true" mandatory="true" name="Robot">
			<or mandatory="true" name="Movement">
				<or name="WaveSurfing">
					<feature name="GuessFactors"/>
					<feature name="TrueSurfing"/>
				</or>
				<feature name="ConstantMovement"/>
				<feature name="WallSmoothing"/>
			</or>
			<or name="Targeting">
				<and name="GuessFactorTargeting">
					<feature name="GaffsTargeting"/>
				</and>
				<feature name="DisplacementVector"/>
				<feature name="StatisticalTargeting"/>
				<feature name="SwarmTargeting"/>
				<feature name="PrayOTheWeak"/>
				<feature name="NoWaste"/>
				<feature name="HeadOn"/>
			</or>
			<or mandatory="true" name="Radar">
				<feature name="SpinningRadar"/>
				<feature name="InfinityLock"/>
			</or>
			<or name="Type">
				<feature name="OneVOne"/>
				<feature name="Melee"/>
			</or>
			<or name="SavingWhen">
				<feature name="AfterRound"/>
				<feature name="AfterMatch"/>
			</or>
			<and name="SavingWhat">
				<feature name="SurfingInformation"/>
				<feature name="GuessFactorInformation"/>
			</and>
			<and name="Visual">
				<feature name="PaintWaves"/>
				<feature name="ChangeRobotColour"/>
			</and>
		</and>
	</struct>
	<constraints>
		<rule>
			<imp>
				<var>SavingWhat</var>
				<var>SavingWhen</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>SavingWhen</var>
				<var>SavingWhat</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>SurfingInformation</var>
				<var>WaveSurfing</var>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>GuessFactorInformation</var>
				<disj>
					<var>GuessFactorTargeting</var>
					<var>GuessFactors</var>
				</disj>
			</imp>
		</rule>
		<rule>
			<imp>
				<var>PaintWaves</var>
				<var>WaveSurfing</var>
			</imp>
		</rule>
	</constraints>
</featureModel>
